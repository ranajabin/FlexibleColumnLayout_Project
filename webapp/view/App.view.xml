<mvc:View
    displayBlock="true"
    height="100%"
    xmlns="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
>
    <FlexibleColumnLayout
        id="flexibleColumnLayout"
        backgroundDesign="Solid"
    >
        <beginColumnPages>
            <mvc:XMLView
                id="beginView"
                viewName="flexiblelayout.flexiblelayoutableproject.view.View1"
            />
        </beginColumnPages>
        <midColumnPages>
            <mvc:XMLView
                id="detailView"
                viewName="flexiblelayout.flexiblelayoutableproject.view.detail"
            />
        </midColumnPages>
        <endColumnPages>
            <mvc:XMLView
                id="endView"
                viewName="flexiblelayout.flexiblelayoutableproject.view.detailDetail"
            />
        </endColumnPages>
    </FlexibleColumnLayout>
</mvc:View>
